#define             <link linkend="TP-ANSI-FG-BLUE--CAPS">TP_ANSI_FG_BLUE</link>
#define             <link linkend="TP-ANSI-FG-MAGENTA--CAPS">TP_ANSI_FG_MAGENTA</link>
#define             <link linkend="TP-ANSI-FG-CYAN--CAPS">TP_ANSI_FG_CYAN</link>
